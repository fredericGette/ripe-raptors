{
  "name": "ripe-raptors",
  "tree": {
    "$className": "DataModel",

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "server": {
        "$path": "src/server"
      }
    },

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "common": {
        "$path": "src/common"
      }
    },
    
    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "client": {
          "$path": "src/client"
        }
      }
    },

    "Workspace": {
      "$className": "Workspace",
      "world": {
        "$path": "src/world"
      }
    },

    "Lighting": {
      "$className": "Lighting",
      "$properties": {
        "Ambient": [
          0.28,
          0.28,
          0.28
        ],
        "EnvironmentDiffuseScale": 1,
        "EnvironmentSpecularScale": 1,
        "GlobalShadows": true,
        "OutdoorAmbient": [
          0.28,
          0.28,
          0.28
        ],
        "Brightness": 3,
        "ShadowSoftness": 0.2,
        "Technology": "ShadowMap"
      }
    },

    "SoundService": {
      "$className": "SoundService",
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
